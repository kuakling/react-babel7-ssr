{
  "name": "react-babel7-ssr",
  "description": "React 16, Webpack 4, Babel 7 and Server side rendering",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/kuakling/react-babel7-ssr.git",
  "author": "Surakit Choodet <kuakling@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node src/index.js",
    "prod": "rimraf dist && cross-env NODE_ENV=production RUN=true node src/index.js",
    "build": "rimraf dist && NODE_ENV=production node src/index.js",
    "serve": "SERVE=true node src/index.js",
    "kill": "kill -9 $(lsof -i:3000 |grep node)"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-universal-import": "^3.1.2",
    "css-loader": "^1.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-hot-loader": "^4.3.12",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.4"
  }
}
