{
  "name": "react-babel7-ssr",
  "description": "React 16, Webpack 4, Babel 7 and Server side rendering",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/kuakling/react-babel7-ssr.git",
  "author": "Surakit Choodet <kuakling@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev & yarn dev-api",
    "dev-api": "nodemon --exec babel-node ./src/restful-api/index-dev.js",
    "dev": "cross-env NODE_ENV=development node src/index.js",
    "prod": "rimraf dist && cross-env NODE_ENV=production RUN=true node src/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production node src/index.js",
    "serve": "SERVE=true node src/index.js",
    "kill": "kill -9 $(lsof -i:4001 |grep node)"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mysql2": "^1.6.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.42.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-universal-import": "^3.1.2",
    "css-loader": "^2.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.5"
  }
}
